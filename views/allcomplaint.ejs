<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../stylesheets/allcomplaint.css">
</head>
<body>
    <nav class="accordion arrows">
        <header class="box">
            <label for="acc-close" class="box-title">Cases menu</label>
        </header>







        <% complaints.reverse().forEach(function(elem){ %>
            <input type="radio" name="accordion" id="cb1" />
            <section class="box">
                <label class="box-title" for="cb1"><%= elem.title %><label for="cb2" class="complabel">Pending</label></label>
                <label class="box-close" for="acc-close"></label>
                <div class="box-content">
                    <br>
                    Your Documents appear here.
                </div>
            </section>
        <% }) %>






        


        
        
        
        <input type="radio" name="accordion" id="acc-close" />
    </nav>
</body>
</html>